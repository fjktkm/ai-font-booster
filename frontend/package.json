{
  "name": "ai-font-booster",
  "version": "1.0.0",
  "description": "機械学習によりフォントの不足しているグリフを補完するアプリケーションです",
  "main": "js/main.js",
  "scripts": {
    "start": "dbus-launch electron . --no-sandbox --disable-gpu",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -wl"
  },
  "build": {
    "appId": "your.id",
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "fontkit": "^2.0.2",
    "node-fetch": "^3.3.2"
  }
}
